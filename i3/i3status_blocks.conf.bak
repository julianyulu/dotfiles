separator_block_width=14

#[music_info]
#command=~/.bin/music_info
#interval=1
#signal=10

[disk]
label=<span font="15"></span>:
markup=pango
command=/usr/lib/i3blocks/disk /
interval=60
color=#1793D1

#[Wifi]
#command=~/.bin/wifi
#signal=1
#interval=10

[battery]
command=~/.config/i3/battery BAT0
interval=30

[load]
label= 
command=/usr/lib/i3blocks/load_average
interval=10
color=#6c71c4

[volume]
label=
command=/usr/lib/i3blocks/volume
interval=once
signal=2
color=#d70a53

[time]
label=
command=date '+%a %d-%m-%y %l:%M:%S %p'
interval=1
color=#50C878